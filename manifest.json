{
  "manifest_version": 2,
  "name": "upflow",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_start",
      "all_frames": true,
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "assets/sprout_16.png"
  },
  "web_accessible_resources": [
    "keep-growing.html"
  ]
}
